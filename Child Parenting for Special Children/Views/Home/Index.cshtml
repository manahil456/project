@{
    ViewData["Title"] = "Home Page";
}




<main>
    <section id="main-title">
        <h1>Welcome to Parenting Special Children</h1>
    </section>
</main>

<section id="gallery">
    <h2>Picture Gallery</h2>
    <div class="gallery-container">
        <div class="gallery-item"><img src="https://child-appeal.org/wp-content/uploads/2022/12/Special-needs-1.jpeg" alt="Image 1"></div>
        <div class="gallery-item"><img src="data:image/jpeg;base64," alt="Image 2"></div>
        <div class="gallery-item"><img src="data:image/jpeg;base64," alt="Image 3"></div>
        <div class="gallery-item"><img src="data:image/jpeg;base64," alt="Image 4"></div>
        <div class="gallery-item"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTaYTahrAJG8vwm91wOcA9cGi-2HqBtuLikTg&s" alt="Image 5"></div>
    </div>
</section>


<section id="support">
    <h2>Support</h2>

    <div class="support-group">
        <h3>Support Groups</h3>
        <ul>
            <li><a href="#">Local Support Groups</a></li>
            <li><a href="#">Online Support Communities</a></li>
            <li><a href="#">Special Interest Groups</a></li>
        </ul>
    </div>

<div class="therapists-directory">
    <h3>Therapists Directory</h3>
    <p>Find specialized therapists for your child's needs:</p>
    <ul>
        <li><a href="#">Speech Therapists</a></li>
        <li><a href="#">Occupational Therapists</a></li>
        <li><a href="#">Behavioral Therapists</a></li>
        <li><a href="#">Physical Therapists</a></li>
    </ul>
</div>

<div class="educational-materials">
    <h3>Educational Materials</h3>
    <ul>
        <li><a href="#">Guides and Handbooks</a></li>
        <li><a href="#">Articles and Research</a></li>
        <li><a href="#">Webinars and Workshops</a></li>
    </ul>
</div>

<div class="gov-nonprofit-resources">
    <h3>Government and Non-Profit Resources</h3>
    <ul>
        <li><a href="#">Government Programs</a></li>
        <li><a href="#">Non-Profit Organizations</a></li>
    </ul>
</div>

<div class="financial-aid">
    <h3>Financial Aid and Grants</h3>
    <ul>
        <li><a href="#">Funding Sources</a></li>
        <li><a href="#">Application Guidelines</a></li>
    </ul>
</div>

<div class="legal-advocacy">
    <h3>Legal and Advocacy Resources</h3>
    <ul>
        <li><a href="#">Special Education Law</a></li>
        <li><a href="#">Advocacy Services</a></li>
    </ul>
</div>

<div class="parenting-tips">
    <h3>Parenting Tips and Strategies</h3>
    <ul>
        <li><a href="#">Daily Life Tips</a></li>
        <li><a href="#">Success Stories</a></li>
    </ul>
</div>

<div class="health-wellness">
    <h3>Health and Wellness</h3>
    <ul>
        <li><a href="#">Medical Resources</a></li>
        <li><a href="#">Wellness Tips</a></li>
    </ul>
</div>
    </section>

    



<style>


    body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        background-color: #116666;
        color:aliceblue;
    }



    #gallery {
        text-align: center;
        margin: 40px 0;
    }

        #gallery h2 {
            font-size: 28px;
            color: #4CAF50;
            margin-bottom: 20px;
        }

    .gallery-container {
        display: flex;
        justify-content: center;
        gap: 20px;
    }

    .gallery-item {
        width: 150px; /* Set the desired width */
        height: 100px; /* Set the desired height */
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

        .gallery-item img {
            width: 150%;
            height: 100%;
            object-fit: cover;
            display: block;
            border-radius: 10px; /* Optional: adds rounded corners */
        }

        .gallery-item:hover {
            transform: scale(1.05);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
        }


    #support {
        text-align: center;
        margin: 40px 0;
    }

        #support h2 {
            font-size: 28px;
            color: #4CAF50;
            margin-bottom: 20px;
        }

    .support-group, .therapists-directory, .educational-materials, .gov-nonprofit-resources, .financial-aid, .legal-advocacy, .parenting-tips, .health-wellness {
        background-color: #173f3f;
        padding: 20px;
        margin: 20px auto;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s ease, box-shadow 0.3s ease;
        border-radius: 10px; /* Optional: adds rounded corners */
        width: 80%; /* Adjust as needed */
        max-width: 600px; /* Adjust as needed */
    }

        .support-group:hover, .therapists-directory:hover, .educational-materials:hover, .gov-nonprofit-resources:hover, .financial-aid:hover, .legal-advocacy:hover, .parenting-tips:hover, .health-wellness:hover {
            transform: scale(1.05);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
        }

        .support-group h3, .therapists-directory h3, .educational-materials h3, .gov-nonprofit-resources h3, .financial-aid h3, .legal-advocacy h3, .parenting-tips h3, .health-wellness h3 {
            font-size: 24px;
            color: #94b3b3;
            margin-bottom: 10px;
        }

        .support-group ul, .therapists-directory ul, .educational-materials ul, .gov-nonprofit-resources ul, .financial-aid ul, .legal-advocacy ul, .parenting-tips ul, .health-wellness ul {
            list-style-type: none;
            padding: 0;
        }

            .support-group ul li, .therapists-directory ul li, .educational-materials ul li, .gov-nonprofit-resources ul li, .financial-aid ul li, .legal-advocacy ul li, .parenting-tips ul li, .health-wellness ul li {
                margin: 10px 0;
            }

                .support-group ul li a, .therapists-directory ul li a, .educational-materials ul li a, .gov-nonprofit-resources ul li a, .financial-aid ul li a, .legal-advocacy ul li a, .parenting-tips ul li a, .health-wellness ul li a {
                    color: #dfecec;
                    text-decoration: none;
                    transition: color 0.3s ease;
                }

                    .support-group ul li a:hover, .therapists-directory ul li a:hover, .educational-materials ul li a:hover, .gov-nonprofit-resources ul li a:hover, .financial-aid ul li a:hover, .legal-advocacy ul li a:hover, .parenting-tips ul li a:hover, .health-wellness ul li a:hover {
                        color #94b3b3;
                    }

  
</style>
